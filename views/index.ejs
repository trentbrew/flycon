      </div>
      <div class="navbar-center" style="width: 50%">
        <input type="text" placeholder="Search Icons" class="input input-bordered input-lg w-full bg-transparent" />
      </div>
      <div class="navbar-end hidden lg:flex">
        <ul class="menu menu-horizontal p-0">
          <li><a href="https://github.com/trentbrew/flycon">GitHub</a></li>
          <li><a href="https://flycon.dev/api/icons">API</a></li>
        </ul>
      </div>
    </nav>

      <main class="w-full p-16">
        <ul class="w-full grid grid-cols-6 gap-4 mt-24">
          <% Object.entries(icons).sort().forEach(icon => { %>
            <li class="aspect-square p-6 border border-opacity-20 flex justify-start items-center gap-4 cursor-pointer hover:bg-base-200 group">
              <span class="group-hover:scale-110"><%- icon[1] %></span>
              <span class="opacity-75"><%= icon[0] %></span>
            </li>
          <% }) %>
        </ul>
      </main>
    </div>
  </body>
</html>
